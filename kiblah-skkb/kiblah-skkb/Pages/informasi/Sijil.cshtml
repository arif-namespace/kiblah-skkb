@page
@model kiblah.Pages.skkb.informasi.SijilModel
@{
    ViewData["Title"] = " Sikil Perakuan (SKKB)";
}

<section style="background-color:#464444" class="breadcrumbs">
    <div class="container">

        <div class="d-flex justify-content-between align-items-center">
            <h2 class="fw-bold text-white">
                SIJIL PERAKUAN SKKB
            </h2>
        </div>

    </div>
</section>

<section class="inner-page mt-4">
    <div class="container">
        <div data-aos="fade-up" class="px-4 py-3 my-3 text-center">
            <img class="d-block mx-auto mb-1 rounded-circle" src="~/assets/logo/LOGO.jpg" alt="" width="100" height="100">
            <h3 class=" fw-bold mt-4">
                PERAKUAN PENDAFTARAN INSTITUT PENDIDIKAN
            </h3>
            <img style="max-height:750px" alt="image" class="img-fluid"
                 src="~/assets/sijil/img1.jpg">
        </div>
    </div>
    <div class="container">
        <div data-aos="fade-up" class="px-4 py-3 my-3 text-center">
            <h3 class=" fw-bold mt-4">
                SIJIL PENARAFAN SEKOLAH SWASTA
            </h3>
            <img style="max-height:750px" alt="image" class="img-fluid"
                 src="~/assets/sijil/img3.jpg">
        </div>
    </div>
    <div class="container">
        <div data-aos="fade-up" class="px-4 py-3 my-3 text-center">
            <h3 class=" fw-bold mt-4">
                SIJIL PENCAPAIAN DALAM PEMERIKSAAN STANDARD KUALITI INSTITUSI PENDIDIKAN SWASTA (SKIPS)
            </h3>
            <img style="max-height:750px" alt="image" class="img-fluid"
                 src="~/assets/sijil/img4.jpg">
        </div>
    </div>
    <div class="container">
        <div data-aos="fade-up" class="px-4 py-3 my-3 text-center">
            <h3 class=" fw-bold mt-4">
                PERAKUAN PENDAFTARAN SEKOLAH AGAMA DAN INSTITUSI PENGAJIAN AGAMA NEGERI KELANTAN
            </h3>
            <img style="max-height:750px" alt="image" class="img-fluid"
                 src="~/assets/sijil/maik1.jpg">
        </div>
    </div>
</section>
